<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<AnchorPane fx:id="mainPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.eggi.filebrowser.gui.MainController">
    <BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="600.0">
        <top>
            <MenuBar fx:id="menuBar" styleClass="menubar" stylesheets="@css/menubar.css" BorderPane.alignment="CENTER">
                <Menu fx:id="file_menu" mnemonicParsing="false" text="File">
                    <MenuItem fx:id="file_menu_item_close" mnemonicParsing="false"
                              onAction="#handleCloseAction" text="Close"/>
                </Menu>
                <Menu fx:id="help_menu" mnemonicParsing="false" text="Help">
                    <MenuItem fx:id="help_menu_item_about" mnemonicParsing="false"
                              onAction="#handleAboutAction" text="About"/>
                </Menu>
            </MenuBar>
        </top>
    </BorderPane>
</AnchorPane>
